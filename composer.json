{
    "name": "chaplean/gitlab-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Gitlab specific features of chaplean-staff",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "https://www.chaplean.coop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chaplean\\Bundle\\GitlabBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Chaplean\\Bundle\\GitlabBundle\\": "Tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.chaplean.coop"
        }
    ],
    "require": {
        "php": ">=7.1",
        "chaplean/doctrine-extensions-bundle": "^4.0 || ^5.0",
        "doctrine/doctrine-bundle": "^1.6",
        "chaplean/form-handler-bundle": "v3.0.0-rc.1 || ^4.0",
        "chaplean/gitlab-client-bundle": "^2.0",
        "symfony/config": "^3.0 || ^4.0",
        "symfony/console": "^3.0 || ^4.0",
        "symfony/dependency-injection": "^3.0 || ^4.0",
        "symfony/yaml": "^3.0 || ^4.0"
    },
    "require-dev": {
        "chaplean/coding-standard": "^1.1",
        "chaplean/unit-bundle": "^7.2",
        "mockery/mockery": "dev-master",
        "symfony/http-kernel": "^2.8 || ^3.0 || ^4.0",
        "symfony/var-dumper": "^2.8 || ^3.0 || ^4.0"
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "sort-packages": true
    }
}
